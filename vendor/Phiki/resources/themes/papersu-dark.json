{
    "name": "papersu-dark",
    "displayName": "Papersu Dark",
    "type": "dark",
    "semanticHighlighting": true,
    "colors": {
        "editor.background": "#2e2e2e",
        "editor.foreground": "#c1bcb4",
        "activityBar.background": "#2e2e2e",
        "activityBar.foreground": "#c1bcb4",
        "editor.lineHighlightBackground": "#363a3c",
        "editorCursor.foreground": "#8fa2e0"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "string.comment"
            ],
            "settings": {
                "foreground": "#bdb7af",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "constant",
                "entity.name.constant",
                "variable.other.constant",
                "variable.other.enummember",
                "variable.language"
            ],
            "settings": {
                "foreground": "#e0c98a"
            }
        },
        {
            "scope": [
                "entity",
                "entity.name"
            ],
            "settings": {
                "foreground": "#e0a8a8"
            }
        },
        {
            "scope": "variable.parameter.function",
            "settings": {
                "foreground": "#c1bcb4"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#e0a8c0"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#8fa2e0"
            }
        },
        {
            "scope": [
                "storage",
                "storage.type"
            ],
            "settings": {
                "foreground": "#8fa2e0"
            }
        },
        {
            "scope": [
                "storage.modifier.package",
                "storage.modifier.import",
                "storage.type.java"
            ],
            "settings": {
                "foreground": "#c1bcb4"
            }
        },
        {
            "scope": [
                "string",
                "punctuation.definition.string",
                "string punctuation.section.embedded source"
            ],
            "settings": {
                "foreground": "#8fcfa8"
            }
        },
        {
            "scope": "support",
            "settings": {
                "foreground": "#8fa2e0"
            }
        },
        {
            "scope": "meta.property-name",
            "settings": {
                "foreground": "#8fcfa8"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#e0a8a8"
            }
        },
        {
            "scope": "variable.other",
            "settings": {
                "foreground": "#c1bcb4"
            }
        },
        {
            "scope": [
                "invalid.broken",
                "invalid.deprecated",
                "invalid.illegal",
                "invalid.unimplemented"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#e0a8a8"
            }
        },
        {
            "scope": [
                "string.regexp",
                "string.regexp.character-class",
                "string.regexp constant.character.escape",
                "string.regexp source.ruby.embedded",
                "string.regexp string.regexp.arbitrary-repitition"
            ],
            "settings": {
                "foreground": "#8fcfa8"
            }
        },
        {
            "scope": "support.constant",
            "settings": {
                "foreground": "#8fcfa8"
            }
        },
        {
            "scope": "support.variable",
            "settings": {
                "foreground": "#8fcfa8"
            }
        },
        {
            "scope": [
                "markup.heading",
                "markup.heading entity.name"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#8fa2e0"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#c1bcb4"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#c1bcb4"
            }
        },
        {
            "scope": [
                "markup.underline"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "markup.strikethrough"
            ],
            "settings": {
                "fontStyle": "strikethrough"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#8fcfa8"
            }
        },
        {
            "scope": [
                "constant.other.reference.link",
                "string.other.link"
            ],
            "settings": {
                "fontStyle": "underline",
                "foreground": "#8fa2e0"
            }
        }
    ]
}
